zone "den.skv" {
    type slave;
    masters { 10.10.10.241; };
    file "slave/den.skv.zone";
};

zone "10.10.10.in-addr.arpa" {
    type slave;
    masters { 10.10.10.241; };
    file "slave/10.10.10.zone";
};
